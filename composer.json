{
    "name": "bjeavons/zxcvbn-php",
    "type": "library",
    "description": "Realistic password strength estimation PHP library based on zxcvbn JS",
    "keywords": [
        "zxcvbn",
        "password",
        "security"
    ],
    "homepage": "https://github.com/bjeavons/zxcvbn-php",
    "license": "MIT",
    "authors": [
        {
            "name": "See contributors",
            "homepage": "https://github.com/bjeavons/zxcvbn-php"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Zxcvbn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\": "test/"
        }
    }
}
